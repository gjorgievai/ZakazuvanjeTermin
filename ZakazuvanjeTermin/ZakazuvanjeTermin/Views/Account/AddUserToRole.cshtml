@model ZakazuvanjeTermin.Models.AddUserToRole
@{
    ViewBag.Title = "AddUserToRole";
}

<h2>Додади улога</h2>

@using (Html.BeginForm("AddUserToRole", "Account"))
{
    <div class="form-group">
        @Html.LabelFor(m => m.Email)
        @Html.TextBoxFor(m => m.Email, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.DropDownListFor(m=>m.Role , new SelectList(Model.roles),new {@class="form-control" })

    </div>
    <button type="submit" class="btn btn-success">Додади</button>

}